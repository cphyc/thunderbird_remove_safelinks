{
  "manifest_version": 2,
  "name": "Safelink removal",
  "description": "Remove safelink protection from emails.",
  "version": "0.0.4c",
  "author": "Corentin Cadiou",
  "applications": {
      "gecko": {
          "id": "safelinks-removal@cphyc.me",
          "strict_min_version": "78.4.0"
      }
  },
  "permissions": [
    "messagesRead",
    "messagesModify"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
